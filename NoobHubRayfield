-- Values

_G.autoTap = true
_G.autoRebirth = true
_G.autoHatch = true
_G.autoclaimRR = true
_G.rebirthAmount = "1"
_G.selectEgg = "Starter"

-- Limited Time Event Values

_G.autoHatchLTE = true
_G.selectLTE = "75M Egg"

-- Functions

function autoTap()
    while _G.autoTap == true do
        game.ReplicatedStorage.Events.Tap:FireServer("Main")
        wait(0.00001)
     end
    end

function autoRebirth()
    while _G.autoRebirth == true do
        game.ReplicatedStorage.Events.Rebirth:FireServer(_G.rebirthAmount)
        wait(0.00001)
     end
    end

function autoHatch()
    while _G.autoHatch == true do
        game.ReplicatedStorage.Events.HatchEgg:InvokeServer({},_G.selectEgg,1)
        wait(0.00001)
    end
end

function autoHatchLTE()
    while _G.autoHatchLTE == true do
        game.ReplicatedStorage.Events.HatchEgg:InvokeServer({},_G.selectLTE,1)
        wait(0.00001)
    end
end

function autoclaimRR()
    while _G.autoclaimRR == true do
        game.ReplicatedStorage.Events.ClaimRandomReward:FireServer()
        wait(3)
    end
end

-- Tabs

local CharacterTab = Window:CreateTab("Character", 7743871002)
local FarmTab = Window:CreateTab("Auto Farm", 7733955511)
local EggsTab = Window:CreateTab("Eggs", 8997385940)
local MiscTab = Window:CreateTab("Miscellaneous", 7743875263)

-- Sections

local CharacterSection = CharacterTab:CreateSection("Walkspeed")

-- Sliders

local Slider = CharacterTab:CreateSlider({
    Name = "Walkspeed",
    Range = {0, 200},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 16,
    Flag = "WalkspeedSlider", 
    Callback = function(Value)
     game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end,
 })

 -- Toggles

local AutoTapToggle = FarmTab:CreateToggle({
    Name = "Auto Tap",
    CurrentValue = false,
    Flag = "AutoTapToggle",
    Callback = function(Value)
        _G.autoTap = Value
        autoTap()
    end,
})

local AutoRebirthToggle = FarmTab:CreateToggle({
    Name = "Auto Rebirth",
    CurrentValue = false,
    Flag = "AutoRebirthToggle",
    Callback = function(Value)
        _G.autoRebirth = Value
        autoRebirth()
    end,
})

local AutoClaimRewardToggle = FarmTab:CreateToggle({
    Name = "Auto Claim Reward",
    CurrentValue = false,
    Flag = "AutoClaimRewardToggle",
    Callback = function(Value)
        _G.autoclaimRR = Value
        autoclaimRR()
    end,
})

local AutoHatchToggle = EggsTab:CreateToggle({
    Name = "Auto Hatch",
    CurrentValue = false,
    Flag = "AutoHatchToggle",
    Callback = function(Value)
        _G.autoHatch = Value
        autoHatch()
    end,
})

local AutoHatchLTEToggle = EggsTab:CreateToggle({
    Name = "Auto Hatch LTE",
    CurrentValue = false,
    Flag = "AutoHatchLTEToggle",
    Callback = function(Value)
        _G.autoHatchLTE = Value
        autoHatchLTE()
    end,
})
 

-- Dropdowns

local EggDropdown = EggsTab:CreateDropdown({
    Name = "Select Egg",
    Options = {"Starter Egg","Wood Egg", "Jungle Egg"},
    CurrentOption = "Starter Egg",
    Flag = "EggDropdown",
    Callback = function(Option)
    end,
})

local LTEggDropdown = EggsTab:CreateDropdown({
    Name = "Limited Egg",
    Options = {"75 Million Egg"},
    CurrentOption = "75 Million Egg",
    Flag = "LTEggDropdown",
    Callback = function(Option)
    end,
})

local AutoRebirthAmountDropdown = FarmTab:CreateDropdown({
    Name = "Auto Rebirth Amount",
    Options = {"1", "5", "10", "20"},
    CurrentOption = "1",
    Flag = "AutoRebirthAmountDropdown",
    Callback = function(Value)
        _G.rebirthAmount = Value
    end,
})

-- Adaptive Inputs
 
local Input = CharacterTab:CreateInput({
    Name = "Custom Walkspeed",
    PlaceholderText = "16",
    RemoveTextAfterFocusLost = false,
    Callback = function(Value)
     game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end,
})

-- Labels

local Label = MiscTab:CreateLabel("Currently in Development.")

-- Load Configuration

Rayfield:LoadConfiguration()
end

